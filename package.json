{
  "name": "gdg-react-workshop",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "electron/main.js",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint ./src --ext \".js,.jsx,.ts,.tsx\" && stylelint \"./src/**/*.css\"",
    "lint:fix": "eslint ./src --ext \".js,.jsx,.ts,.tsx\" --fix && stylelint \"./src/**/*.css\" --fix",
    "start": "nf start -p 3000",
    "test": "react-scripts test",
    "electron": "tsc electron/main.ts && electron .",
    "electron-start": "ts-node --skip-project electron/connect-electron.ts",
    "react-start": "cross-env BROWSER=none react-scripts start"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "classnames": "^2.2.6",
    "localforage": "^1.7.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-stack-grid": "^0.7.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.2.3",
    "@types/material-ui": "^0.21.7",
    "@types/node": "14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "cross-env": "^7.0.2",
    "electron": "^9.0.0",
    "electron-builder": "^22.7.0",
    "eslint": "^7.1.0",
    "eslint-plugin-react": "^7.20.0",
    "foreman": "^3.0.1",
    "stylelint": "^13.5.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "build": {
    "appId": "com.electron.gdg-react-workshop",
    "directories": {
      "buildResources": "public"
    }
  }
}
