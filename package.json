{
  "name": "gdg-react-workshop",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "electron/main.js",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "tslint \"src/**/*.ts{,x}\" && stylelint \"./src/**/*.css\"",
    "lint:fix": "tslint 'src/**/*.ts{,x}' --fix && stylelint \"./src/**/*.css\" --fix",
    "start": "nf start -p 3000",
    "test": "react-scripts test",
    "electron": "tsc electron/main.ts && electron .",
    "electron-start": "ts-node --skip-project electron/connect-electron.ts",
    "react-start": "cross-env BROWSER=none react-scripts start"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^4.5.1",
    "classnames": "^2.2.6",
    "localforage": "^1.7.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-stack-grid": "^0.7.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/electron": "^1.6.10",
    "@types/electron-builder": "^2.8.0",
    "@types/jest": "^24.0.25",
    "@types/material-ui": "^0.21.7",
    "@types/node": "12.12.23",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/vfile-message": "^2.0.0",
    "cross-env": "^6.0.3",
    "electron": "^7.1.7",
    "electron-builder": "^21.2.0",
    "foreman": "^3.0.1",
    "stylelint": "^12.0.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^4.0.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "build": {
    "appId": "com.electron.gdg-react-workshop",
    "directories": {
      "buildResources": "public"
    }
  }
}
